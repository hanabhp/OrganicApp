<!DOCTYPE html>
<html direction="rtl" dir="rtl" style="direction: rtl">
<!--begin::Head-->
<head>

    <title>Admin Panel | @ViewData["Title"]</title>
    <partial name="_Styles" />

    <link rel="shortcut icon" href="/favicon.ico" />

    <script src="https://www.recaptcha.net/recaptcha/api.js?onload=onloadCallback&render=explicit"
            async defer>
    </script>
</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_body"
      class="header-fixed header-mobile-fixed subheader-enabled ltr
      subheader-fixed aside-enabled aside-fixed aside-minimize-hoverable page-loading">

<partial name="_Header" />

<div class="d-flex flex-column flex-root">

    <div class="d-flex flex-row flex-column-fluid page">

        <partial name="_Aside" />

        <!--begin::Main-->
        <!--begin::Wrapper-->
        <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
            <partial name="_MainHeader" />
            <!--begin::Content-->
            <div class="content  d-flex flex-column flex-column-fluid" id="kt_content">
                @RenderBody()
            </div>
            <!--end::Content-->
            <partial name="_MainFooter" />
        </div>
        <!--end::Wrapper-->

    </div>
</div>

<partial name="_ScrollTop" />

<partial name="_Js" />
@await RenderSectionAsync("Scripts", required: false)


</body>
</html>